{
  "name": "frontend-template",
  "version": "1.2.0",
  "description": "This is a starter kit of the Web front-end development.",
  "author": "akihiro kato",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "web",
    "frontend",
    "starter"
  ],
  "babel": {
    "presets": [
      "latest",
      "react",
      "es2015",
      "stage-0"
    ],
    "env": {
      "development": {
        "presets": [
          "power-assert",
          "react",
          "es2015",
          "stage-0"
        ]
      }
    }
  },
  "esdoc": {
    "source": "./src/js",
    "destination": "./esdoc",
    "test": {
      "type": "mocha",
      "source": "./test"
    }
  },
  "scripts": {
    "test": "mocha --compilers js:espower-babel/guess test/**/*.test.js",
    "start": "gulp watch",
    "esdoc": "esdoc",
    "server": "gulp server",
    "build:css": "gulp css",
    "build:js": "gulp js",
    "release": "gulp release"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "milkcocoa": "^0.7.0",
    "mongoose": "^4.11.10",
    "normalize.css": "^5.0.0",
    "numeral": "^2.0.6",
    "perfect-scrollbar": "^0.8.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-modal": "^2.3.2",
    "react-qr-reader": "^2.0.0",
    "react-router": "^2.4.0",
    "socket.io": "^2.0.3",
    "store": "^2.0.12",
    "sweetalert": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.6",
    "browserify": "^13.3.0",
    "del": "^2.2.2",
    "esdoc": "^0.5.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.3.2",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.4.0",
    "gulp-pleeease": "^2.0.2",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.8",
    "gulp-watchify": "^0.6.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2",
    "pretty-hrtime": "^1.0.3",
    "run-sequence": "^1.2.2",
    "vinyl-buffer": "^1.0.0",
    "watchify": "^3.8.0"
  }
}
